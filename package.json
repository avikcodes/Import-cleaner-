{
  "name": "tidy-imports",
  "displayName": "Tidy Imports",
  "icon": "media/icon.png",
  "publisher": "avikghosh",
  "description": "Safely remove unused import statements from JavaScript and TypeScript files",
  "version": "1.0.0",
  "engines": {
    "vscode": "^1.74.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/avikcodes/Tidy-Imports.git"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:tidyImports.clean"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "tidyImports.clean",
        "title": "Tidy Imports: Remove Unused Imports"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/node": "16.x",
    "@types/vscode": "^1.74.0",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "fast-glob": "^3.3.3"
  }
}